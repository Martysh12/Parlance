{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Vicr123Accounts": {
    "DbusConnectionPath": "unix:path=/var/vicr123-accounts/vicr123-accounts-bus"
  },
  "Parlance": {
    "RepositoryDirectory": "$HOME/Parlance/vcs",
    "DatabaseConnectionString": "Host=localhost;Username=parlance;Password=secret;Database=parlancedb",
    "UseDummyAuthenticationService": false
  },
  "fido2": {
    "Origins": [
      "https://parlance.local:44402",
      "https://localhost:44402"
    ],
    "ServerDomain": "parlance.local"
  },
  "rateLimiting": {
    "PermitLimit": 500,
    "Window": 3600,
    "SegmentsPerWindow": 4
  }
}
